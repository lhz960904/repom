{
  "compilerOptions": {
    "target": "ESNext",                             
    "module": "commonjs",                           
    "moduleResolution": "node",                     
    "resolveJsonModule": true,                       
    "outDir": "./dist",                           
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,        
    "strict": true,                                    
    "skipLibCheck": true                              
  }
}
